country_code={{ dhcp.country_code }}
interface={{ hostapd.interface|default('wlan0') }}
driver=nl80211
ssid={{ hostapd.ssid }}
hw_mode=g
channel={{ hostapd.channel }}
wmm_enabled=0
macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=0
wpa=2
wpa_key_mgmt=WPA-PSK
wpa_passphrase={{ hostapd.key }}
wpa_pairwise=TKIP
rsn_pairwise=CCMP

ctrl_interface=/var/run/hostapd
ctrl_interface_group=0
